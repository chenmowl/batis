<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ymtz.batis.GoodsMapper">
	<select id="selectActive" resultType="ymtz.batis.Goods">
		select * from gs_goods where
		category_id = #{category_id}
	</select>
</mapper>